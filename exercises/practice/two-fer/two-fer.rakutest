#!/usr/bin/env raku
use Test;
use lib $?FILE.IO.dirname; #`[Look for the module inside the same directory as this test file.]
use TwoFer;

is(
    two-fer(),
    "One for you, one for me.",
    "no name given",
);

is(
    two-fer("Alice"),
    "One for Alice, one for me.",
    "a name given",
);

is(
    two-fer("Bob"),
    "One for Bob, one for me.",
    "another name given",
);

done-testing;
